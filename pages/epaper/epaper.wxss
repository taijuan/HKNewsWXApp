  height: 100%;
  box-sizing: content-box;
  display: flex;
  flex-direction: column;
  background-color: #f5f5f5;
}

.epaper-header {
  box-sizing: content-box;
  width: 100%;
  height: 506rpx;
  position: fixed;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* justify-content: center; */
}
.image-epaper {
  box-sizing: content-box;
  width: 100%;
  height: 506rpx;
  position: fixed;
}
.iamge-text{
  width: 556rpx;
