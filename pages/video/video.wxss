/* pages/video/video.wxss */

.video-list {
  width: 100%;
  box-sizing: content-box;
}

.video-container {
  margin: 16rpx;
  background: #f5f5f5;
  display: flex;
  flex-direction: column;
  border-radius: 10rpx;
  border-color: #f00;
  border-width: 2rpx;
  z-index: 99;
  box-sizing: content-box;
}

.video-image {
  width: 100%;
  height: 404rpx;
}

.video-name {
  padding: 16rpx;
}
